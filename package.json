{
  "name": "cs-netbanking-sdk",
  "version": "0.1.1",
  "description": "CS Netbanking SDK for the CSAS Web Api",
  "main": "dist/cs-netbanking-sdk.node.js",
  "scripts": {
    "dist": "yarn run build && node node_modules/cs-core-sdk/tooling/dist",
    "build": "node node_modules/cs-core-sdk/tooling/clean && webpack && webpack --target-browser && yarn run generate-tsd",
    "test": "yarn run build && yarn run test-node-run && yarn run test-browser-run",
    "test-node": "yarn run build && yarn run test-node-run",
    "test-browser": "yarn run build  && yarn run test-browser-run",
    "generate-tsd": "node node_modules/cs-core-sdk/tooling/generate-tsd cs-netbanking-sdk CSNetbankingSDK netbanking",
    "test-node-run": "webpack --config ./webpack.test.config.js && jasmine ./spec/build/tests.node.js",
    "test-browser-run": "webpack --config ./webpack.test.config.js --target-browser && node ./node_modules/karma/bin/karma start karma.conf.js --single-run",
    "preversion": "yarn run build && yarn run test-node-run && yarn run test-browser-run",
    "version": "yarn run dist && git add -A dist",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ceskasporitelna/cs-netbanking-js.git"
  },
  "author": "Petr Ba≈æout <petr.bazout@applifting.cz>",
  "license": "Proprietary",
  "bugs": {
    "url": "https://github.com/Ceskasporitelna/cs-netbanking-js/issues"
  },
  "homepage": "https://github.com/Ceskasporitelna/cs-netbanking-js#readme",
  "devDependencies": {
    "dts-generator": "1.6.0-pre.1",
    "jasmine": "^2.4.1",
    "jasmine-ajax": "^3.2.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.19",
    "karma-chrome-launcher": "^0.2.1",
    "karma-jasmine": "^0.3.6",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "json-loader": "^0.5.3",
    "nock": "^2.17.0",
    "replace": "^0.3.0",
    "serve": "^1.4.0",
    "shelljs": "^0.5.3",
    "ts-loader": "^0.7.2",
    "typescript": "^1.7.5",
    "underscore": "^1.8.3",
    "webpack": "^1.12.9"
  },
  "dependencies": {
    "axios": "^0.8.1",
    "cs-core-sdk": "https://github.com/Ceskasporitelna/cs-core-sdk-js.git#v0.7.4",
    "node-uuid": "^1.4.3",
    "source-map-support": "^0.3.3",
    "standard-error": "^1.1.0"
  }
}
